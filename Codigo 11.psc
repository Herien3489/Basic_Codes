Algoritmo Condicional
	//Definir las variables
	Definir num Como Real;
	//Programa de negativo o positivo
	//Pedimos el numero
	Escribir "Escribe un numero";
	leer num;
	//Hacemos el comdicional
	si num>=0 Entonces
		Escribir num, " Es positivo";
	SiNo
		Escribir num, " Es negativo";
	FinSi
	
FinAlgoritmo
